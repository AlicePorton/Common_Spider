<script type="text/javascript">
$(function(){
  var weather_box_small = $("#weather_box_small");
  $.ajax({  
        type: "GET",  
        async: true,  
        url: "http://bmfw.www.gov.cn/ZFW-AccessPlatform/front/weather/observe_ky.do?callback=?",
        data:{},
        dataType: "jsonp",  
        //jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(一般默认为:callback)   
        success: function (data) { 
          if(weather_box_small.length > 0){
          var html_2 = "<div style='width:110px;height:25px;position:relative;'>"
                  +"<img style='float: left;width:25px;height:25px;' src='http://bmfw.www.gov.cn"+data.weatherPicture_small+"'>"
                +"<p style='margin:0;display: block;font-size: 16px;float: left;height: 25px;line-height: 25px;font-family: Microsoft YaHei;margin-left: 7px;'>"+data.temperature+"℃ "+data.city+"</p>"
                +"</div>";
          $("#weather_box_small").html(html_2);
        }  
        },  
        error: function (XMLHttpRequest, textStatus, errorThrown) {  
          
        }  
    });  
});
</script>
