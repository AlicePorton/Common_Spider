<script type="text/javascript">
      (function ($) {
        var url='http://sousuo.gov.cn/s.htm?t=zhengce&q=';
      
        $('.goEnter').on('click', function () {
          
          var $changvalue=$('.form_text').val();
          
          if ($changvalue === ''||$changvalue ==='搜索关键字') {
            return false;
          }else{
            window.open(url+encodeURIComponent($changvalue));
          }
          
        });
        
      
        $('.form_text').on('keypress', function (ev) { 
          
          var oEvent=ev||event;
          
          console.log(oEvent.keyCode)
           if (oEvent.keyCode === 13) { 
            console.log(888)
            $('.goEnter').trigger('click');
            
           }
        });
      
      })(jQuery)
    </script>
    
<!--政策栏目下面图片鼠标hover换图片效果-->
<script type="text/javascript">
  ;(function ($) {
    
    var originalImgUrl=[
      'http://www.gov.cn/govweb/xhtml/2016gov/20181121shouyezhengcepage/images/zt1.jpg',
      'http://www.gov.cn/govweb/xhtml/2016gov/20181121shouyezhengcepage/images/zt2.jpg'
    ];
    var hoverImgUrl=[
      'http://www.gov.cn/govweb/xhtml/2016gov/20181121shouyezhengcepage/images/double_innovation_policy_img.jpg',
      'http://www.gov.cn/govweb/xhtml/2016gov/20181121shouyezhengcepage/images/promote_folk_img.jpg'
    ];
    
    
    $('.dys_scfw_bg').on('mouseover', 'img', function () {
      
        var index=$('.dys_scfw_bg a img').index($(this));
        
        $(this).attr('src', hoverImgUrl[index]);
        
        if (index===0) {
          index=1;
          $(this).parent().siblings().find('img').attr('src', originalImgUrl[index]);
        }else{
          index=0;
          $(this).parent().siblings().find('img').attr('src', originalImgUrl[index]);
        }
        
        
    });
    $('.dys_scfw_bg').on('mouseout', function () {
      
      $.each(originalImgUrl, function(i, elem) {
          $('.dys_scfw_bg a img').eq(i).attr('src', elem);
      });
      
    });
    
  })(jQuery);
</script>
