<script type="text/javascript">
  (function ($) {
      var $fuwulenght=$('.two_yyphb_box li').length;
      
      switch ($fuwulenght){
        case 1:$('.two_yyphb_box li').eq(0).css('border-bottom', 'none');
          break;
        case 2:
          $('.two_yyphb_box li').eq(1).css('border-bottom', 'none');
          
          break;
        case 3:
          $('.two_yyphb_box li').eq(2).css('border-bottom', 'none');
          
          break;
        default:
          break;
      }
  })(jQuery);
</script>
