     <script type="text/javascript">
      $(function(){
       $('#q').on('focus',function () {
           $(this).css("color","#000");
          if($(this).val()=='请输入你想找的服务'){
        
            $(this).val('');
          
            
        }
       });
      $("#pc_submit").click(function(){
          var q=$("#q").val();
          var category=$(".zhetie h3 a.on").attr("name");
          var search_url="http://search.www.gov.cn/search/fw/cateSearch.do?webid=1&category="+category+"&q="+encodeURIComponent(q);
          if(q == '请输入你想找的服务'){
            q = '';
          }
          if(category==""||category==null){
            search_url="http://search.www.gov.cn/search/fw/search.do?webid=1&q="+encodeURIComponent(q);

          }
          window.open(search_url,"_blank")
        })
      $(".pc_search #q").keydown(function(){
          if(event.keyCode == "13"){
            $("#pc_submit").click();
          }
        })
        $.ajax({
            type: "get",
            url: "http://search.www.gov.cn/search/search/plugin/wordranklist.do?size=4&webid=1",
            async: true,
            dataType:'jsonp',
            crossDomain: true,
            success: function(response) {
              var html = '';
              var count = 0;
              $.each(eval(response),function(index,value){
                html += "<a target='_blank' href='http://search.www.gov.cn/search/fw/search.do?webid=1&q="+encodeURI(value)+"'>"+value+"</a>";
                count++;
              });
              $('.a').append(html);
            }
          });
        
      })
      
    </script>
      
