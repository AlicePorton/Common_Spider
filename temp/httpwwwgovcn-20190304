 <script src="/govweb/xhtml/2016gov/public/pic20181229/2018scrollImg.js" type="text/javascript" charset="utf-8"></script>
<script type="text/javascript">

$(function(){   
    /*focus*/
    var sliderElement = $('.slider-carousel');
    sliderElement.slidesjs({
        play: {
            auto: true,
            effect: 'fade'
        },
        navigation: {
            effect: "fade"
        },
        pagination: {
            effect: "fade"
        },
        effect: {
            slide: {
                speed: 400
            }
        }
    });
    var slidernav = sliderElement.find('.slidesjs-navigation');
    sliderElement.hover(function() {
        slidernav.stop().show()
    },function() {
        slidernav.stop().hide()
    });
  
  slidernav.hover(function(){
    $(this).show();
  }),function(){
    $(this).show();
  }
  

    $(".flexslider").each(function(){
        $(this).hover(function(){
            $(this).find(".flex-direction-nav").fadeIn("slow")
        },function(){
            $(this).find(".flex-direction-nav").hide()
        })
    })
});

</script>
